{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "rabbitmq:start": {
      "cache": false,
      "outputs": []
    },
    "rabbitmq:stop": {
      "cache": false,
      "outputs": []
    },
    "rabbitmq:reset": {
      "cache": false,
      "outputs": []
    },
    "ocr:start": {
      "cache": false,
      "outputs": []
    },
    "ocr:stop": {
      "cache": false,
      "outputs": []
    },
    "api:dev": {
      "cache": false,
      "dependsOn": ["rabbitmq:start", "ocr:start"]
    },
    "server:dev": {
      "cache": false,
      "dependsOn": ["rabbitmq:start", "ocr:start"]
    },
    "web:dev": {
      "cache": false,
      "dependsOn": ["api:dev", "server:dev"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["web:dev", "server:dev", "api:dev"]
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
}
