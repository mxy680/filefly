{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "rabbitmq#start": {
      "cache": false,
      "outputs": []
    },
    "rabbitmq#stop": {
      "cache": false,
      "outputs": []
    },
    "weaviate#start": {
      "cache": false,
      "outputs": []
    },
    "weaviate#stop": {
      "cache": false,
      "outputs": []
    },
    "web#dev": {
      "dependsOn": ["rabbitmq#start", "weaviate#start"],
      "cache": false
    },
    "server#start": {
      "dependsOn": ["rabbitmq#start", "weaviate#start"],
      "cache": false
    },
    "api#dev": {
      "dependsOn": ["rabbitmq#start", "weaviate#start"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["web#dev", "server#start", "api#dev"]
    }
  }
}
